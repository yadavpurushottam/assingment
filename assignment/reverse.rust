#include <iostream>
#include <string>

using namespace std;



int main() {
    string str = "Hello, world!";
    cout <<  str << endl;

    int left = 0;
    int right = str.length() - 1;

    while (left < right) {
        swap(str[left], str[right]);
        left++;
        right--;
    }
    cout << str << endl;

    return 0;
}
